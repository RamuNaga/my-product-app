{
  "name": "backend-company",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/company/src",
  "projectType": "library",
  "tags": ["type:feature", "scope:backend", "area:company"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/company",
        "tsConfig": "libs/backend/company/tsconfig.lib.json",
        "packageJson": "libs/backend/company/package.json",
        "main": "libs/backend/company/src/index.ts",
        "assets": ["libs/backend/company/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/backend/company/jest.config.ts"
      }
    }
  }
}
