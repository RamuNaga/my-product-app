{
  "name": "frontend-data-access",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/data-access/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [
    "type:buildable",
    "type:feature",
    "platform:angular",
    "scope:frontend"
  ],
  "implicitDependencies": ["frontend-core", "frontend-graphql-types"],
  "targets": {
    // "build": {
    //   "executor": "@nx/angular:package",
    //   "outputs": ["{options.outputPath}"],
    //   "options": {
    //     "tsConfig": "libs/frontend/data-access/tsconfig.lib.json",
    //     "project": "libs/frontend/data-access/ng-package.json"
    //   }
    // },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/frontend/data-access",
        "main": "libs/frontend/data-access/src/index.ts",
        "tsConfig": "libs/frontend/data-access/tsconfig.lib.json",
        "assets": ["libs/frontend/data-access/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/frontend/data-access/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
