{
  "name": "@my-product-app/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:all": "concurrently -n gateway,product,user -c blue,green,magenta \"nx serve api-gateway\" \"nx serve backend-product-service\" \"nx serve backend-user-service\"",
    "build:all": "nx run-many --target=build --projects=api-gateway,backend-product-service,backend-user-service --parallel",
    "prisma:generate": "pnpm prisma generate --schema=libs/prisma/prisma/schema.prisma",
    "prisma:prepare": "nx run-many --target=generate --projects=prisma && nx build prisma && nx build user",
    "clean": "rimraf dist",
    "build": "nx build",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "graphql:generate": "nx run user:graphql-schema"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~19.2.0",
